{
  "name": "remix-vite-template",
  "version": "1.5.2",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "bun run clean && bun ./server.ts",
    "build": "bun run clean && remix vite:build",
    "build:icons": "tsx ./scripts/build-icons.ts",
    "start": "cross-env NODE_ENV=production bun ./server.ts",
    "start:mocks": "cross-env NODE_ENV=production MOCKS=true bun server.ts",
    "test": "vitest",
    "typecheck": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run",
    "coverage": "vitest run --coverage",
    "test:e2e": "bun run test:e2e:dev --silent",
    "test:e2e:dev": "playwright test --ui",
    "pretest:e2e:run": "bun run build",
    "test:e2e:run": "cross-env CI=true playwright test",
    "test:e2e:install": "npx playwright install --with-deps chromium",
    "clean": "rimraf build",
    "nuke": "bun run clean && rimraf node_modules package-lock.json && bun i",
    "act": "doppler run -- act -s FLY_API_TOKEN --container-architecture linux/amd64"
  },
  "dependencies": {
    "@elysiajs/static": "^1.1.1",
    "@radix-ui/react-avatar": "^1.1.1",
    "@radix-ui/react-collapsible": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-icons": "^1.3.1",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.3",
    "@remix-run/node": "^2.8.1",
    "@remix-run/react": "^2.8.1",
    "@remix-run/serve": "^2.13.1",
    "chalk": "^5.3.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "elysia": "^1.1.24",
    "elysia-remix": "^0.0.2",
    "isbot": "^5.1.1",
    "logestic": "^1.2.4",
    "lucide-react": "^0.454.0",
    "morgan": "^1.10.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "remix-themes": "^1.5.1",
    "remix-typedjson": "^0.4.1",
    "remix-utils": "^7.7.0",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.5.2",
    "@playwright/test": "^1.42.1",
    "@remix-run/dev": "^2.8.1",
    "@remix-run/eslint-config": "^2.8.1",
    "@remix-run/testing": "^2.8.1",
    "@tailwindcss/typography": "^0.5.15",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/bun": "^1.1.13",
    "@types/eslint": "^8.56.5",
    "@types/fs-extra": "^11.0.4",
    "@types/morgan": "^1.9.9",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.3.1",
    "autoprefixer": "^10.4.18",
    "bun-types": "^1.1.34",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "execa": "^8.0.1",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10",
    "jsdom": "^24.0.0",
    "node-html-parser": "^6.1.12",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "remix-development-tools": "^3.7.4",
    "remix-flat-routes": "^0.6.4",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.1",
    "typescript": "5.1.6",
    "vite": "^5.1.4",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.3.1"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "eslintIgnore": [
    "node_modules",
    "build",
    "public/build"
  ]
}